{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/global",
      "dest": "/api/global.ts"
    },
    {
      "src": "/api/coins/markets",
      "dest": "/api/coins/markets.ts"
    },
    {
      "src": "/api/coins/([^/]+)/market_chart",
      "dest": "/api/coins/[id]/market_chart.ts?id=$1"
    },
    {
      "src": "/api/coins/([^/]+)",
      "dest": "/api/coins/[id].ts?id=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ]
}